<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_pressed="true"><shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <!-- 圆角，你也可以对不同的角设置不同的数值 -->

            <solid android:color="@color/white" />
            <!-- 形状的填充色 -->

            <stroke android:width="@dimen/DIMEN_2PX" android:color="@color/blue_bg" />
            <!-- 边框宽度和颜色 -->
        </shape></item>
    <item android:state_enabled="true" android:state_pressed="false"><shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <!-- 圆角，你也可以对不同的角设置不同的数值 -->

            <solid android:color="#00EA5414" />
            <!-- 形状的填充色 -->

            <stroke android:width="@dimen/DIMEN_2PX" android:color="@color/blue_bg" />
            <!-- 边框宽度和颜色 -->
        </shape></item>

</selector>